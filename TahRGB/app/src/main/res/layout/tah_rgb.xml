<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="10dp"
    android:background="@drawable/appbackground"
    android:clickable="true">


    <RelativeLayout
        android:id="@+id/relative6"
        android:layout_width="330dp"
        android:layout_height="400dp"
        android:alpha="0.2"
        android:background="#fff8e1"
        android:paddingBottom="20dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="20dp"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true">


    </RelativeLayout>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:id="@+id/relativeLayout3">

        <TableRow
            android:id="@+id/row1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:gravity="center">

            <Button
                android:id="@+id/imgaqua"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:layout_toLeftOf="@+id/imgsalmon"
                android:layout_toStartOf="@+id/imgsalmon"
                android:background="@color/aqua" />

            <Button
                android:id="@+id/imgsalmon"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:layout_toLeftOf="@+id/imgbanana"
                android:layout_toStartOf="@+id/imgbanana"
                android:background="@color/salmon" />

            <Button
                android:id="@+id/imgbanana"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@color/banana" />


            <Button
                android:id="@+id/imggrass"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toEndOf="@+id/imgbanana"
                android:layout_toRightOf="@+id/imgbanana"
                android:background="@color/grass" />

        </TableRow>

        <TableRow
            android:id="@+id/row2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/row1"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:gravity="center">

            <Button
                android:id="@+id/imggrape"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:background="@color/grape" />

            <Button
                android:id="@+id/imgtangerine"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:background="@color/tangerine" />

            <Button
                android:id="@+id/imgice"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@color/ice" />

            <Button
                android:id="@+id/imgclover"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:background="@color/clover" />

        </TableRow>

        <TableRow
            android:id="@+id/row3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/row2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:gravity="center">

            <Button
                android:id="@+id/imgStrawberry"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:background="@color/strawberry" />

            <Button
                android:id="@+id/imgHonwydew"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:layout_marginRight="10dp"
                android:background="@color/honwydew" />

            <Button
                android:id="@+id/imgSnow"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:background="@color/snow" />

            <Button
                android:id="@+id/imgPlum"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:background="@color/plum" />

        </TableRow>

        <TableRow
            android:id="@+id/row4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/row3"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="20dp"
            android:gravity="center">

            <Button
                android:id="@+id/imgranbow"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:layout_alignParentTop="true"
                android:text="Rainbow"
                android:gravity="center"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:background="@drawable/appbackground" />

            <Button
                android:id="@+id/imgoff"
                android:layout_width="120dp"
                android:layout_height="60dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="30dp"
                android:gravity="center"
                android:background="#739174"
                android:textAllCaps="false"
                android:text="Off"
                android:textColor="#ffffff" />

        </TableRow>
    </RelativeLayout>
</RelativeLayout>
